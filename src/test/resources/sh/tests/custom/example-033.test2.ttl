@prefix ex: <http://example.org#> .
@prefix sh: <http://www.w3.org/ns/shacl#> .

ex:v0 ex:P ex:v1 .
ex:v0 ex:P ex:v2 .
ex:v1 ex:P ex:v1 .
ex:v1 ex:P ex:v2 .
ex:v2 ex:P ex:v2 .
ex:v2 ex:P ex:v1 .

ex:s0
        a sh:NodeShape ;
        sh:targetNode ex:v0 ;
        sh:property [
                sh:path ex:P ;
             	sh:qualifiedValueShape ex:s0 ;
                sh:qualifiedMinCount 2;
        ] .
