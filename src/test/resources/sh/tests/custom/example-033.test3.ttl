@prefix dbo: <http://dbpedia.org/ontology/> .
@prefix dbr: <http://dbpedia.org/resource/> .
@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix : <http://example.org/> .

:ActorShapeA a sh:PropertyShape;
    sh:path dbo:birthPlace;
    sh:minCount 1.
:ActorShapeC a sh:PropertyShape;
     sh:path dbo:occupation;
	sh:qualifiedValueShape [ sh:hasValue dbr:Actor ] ;
     sh:qualifiedMinCount 1 .
:ActorShapeD a sh:PropertyShape;
     sh:path dbo:imdbId;
     sh:minCount 1.
:ActorShapeE a sh:PropertyShape;
     sh:path [ sh:inversePath dbo:starring ];
     sh:node :MovieShape .
         
:ActorShape a sh:NodeShape;
   sh:property :ActorShapeA;
   sh:property :ActorShapeC;
   sh:property :ActorShapeD;
   sh:property :ActorShapeE.

:MovieShapeA a sh:PropertyShape;
    sh:path dbo:writer;
    sh:minCount 1.
:MovieShapeB a sh:PropertyShape;
    sh:path dbo:musicComposer;
    sh:minCount 1.
:MovieShapeC a sh:PropertyShape;
    sh:path dbo:director;
    sh:minCount 1.
:MovieShapeD a sh:PropertyShape;
    sh:path dbo:imdbId;
    sh:minCount 1;
    sh:maxCount 1.
:MovieShapeE a sh:PropertyShape;
    sh:path dbo:starring;
	sh:qualifiedValueShape :MovieShapeEE ;
    sh:qualifiedMinCount 1.
:MovieShapeEE a sh:NodeShape ;
    sh:node :ActorShape .


:MovieShape a sh:NodeShape;
  sh:targetNode <http://nl.dbpedia.org/resource/The_Jazz_Singer>;
  sh:property :MovieShapeA;
  sh:property :MovieShapeB;
  sh:property :MovieShapeC;
  sh:property :MovieShapeD;
  sh:property :MovieShapeE.